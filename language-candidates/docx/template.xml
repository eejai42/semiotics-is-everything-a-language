<?xml version="1.0" encoding="UTF-8"?>
<!--
  ERB Schema - DOCX Template (Word ML)
  ====================================
  Mirrors the PostgreSQL functions from postgres/02-create-functions.sql
  Source: effortless-rulebook/effortless-rulebook.json

  This is a simplified Word ML template showing the structure.
  For actual DOCX generation, use this as a reference for content placeholders.

  DAG Execution Order:
    Level 0: Raw fields
    Level 1: categoryContainsLanguage, hasGrammar, relationshipToConcept, familyFuedQuestion
    Level 2: isAFamilyFeudTopAnswer (depends on categoryContainsLanguage)
    Level 3: familyFeudMismatch (depends on isAFamilyFeudTopAnswer)
-->
<w:document xmlns:w="http://schemas.openxmlformats.org/wordprocessingml/2006/main">
  <w:body>

    <!-- Title -->
    <w:p>
      <w:pPr><w:pStyle w:val="Title"/></w:pPr>
      <w:r><w:t>ERB Language Candidates Report</w:t></w:r>
    </w:p>

    <!-- Subtitle -->
    <w:p>
      <w:pPr><w:pStyle w:val="Subtitle"/></w:pPr>
      <w:r><w:t>Generated from effortless-rulebook.json</w:t></w:r>
    </w:p>

    <!-- Core Language Definition -->
    <w:p>
      <w:pPr><w:pStyle w:val="Heading1"/></w:pPr>
      <w:r><w:t>Core Language Definition</w:t></w:r>
    </w:p>
    <w:p>
      <w:r><w:t>A candidate x is classified as a Language if:</w:t></w:r>
    </w:p>
    <w:p>
      <w:pPr><w:pStyle w:val="CodeBlock"/></w:pPr>
      <w:r><w:t>Language(x) := HasSyntax(x) ∧ RequiresParsing(x) ∧ Meaning_Is_Serialized(x) ∧ IsOngologyDescriptor(x)</w:t></w:r>
    </w:p>

    <!-- Language Candidates Section -->
    <w:p>
      <w:pPr><w:pStyle w:val="Heading1"/></w:pPr>
      <w:r><w:t>Language Candidates</w:t></w:r>
    </w:p>

    <!-- Template for each candidate (repeat this block) -->
    <!-- {{#each candidates}} -->
    <w:p>
      <w:pPr><w:pStyle w:val="Heading2"/></w:pPr>
      <w:r><w:t>{{name}}</w:t></w:r>
    </w:p>

    <w:tbl>
      <w:tblPr>
        <w:tblStyle w:val="TableGrid"/>
        <w:tblW w:w="5000" w:type="pct"/>
      </w:tblPr>

      <!-- Raw Fields (Level 0) -->
      <w:tr>
        <w:tc><w:p><w:r><w:t>ID</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{{language_candidate_id}}</w:t></w:r></w:p></w:tc>
      </w:tr>
      <w:tr>
        <w:tc><w:p><w:r><w:t>Category</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{{category}}</w:t></w:r></w:p></w:tc>
      </w:tr>
      <w:tr>
        <w:tc><w:p><w:r><w:t>Has Syntax</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{{has_syntax}}</w:t></w:r></w:p></w:tc>
      </w:tr>
      <w:tr>
        <w:tc><w:p><w:r><w:t>Requires Parsing</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{{requires_parsing}}</w:t></w:r></w:p></w:tc>
      </w:tr>
      <w:tr>
        <w:tc><w:p><w:r><w:t>Meaning Is Serialized</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{{meaning_is_serialized}}</w:t></w:r></w:p></w:tc>
      </w:tr>
      <w:tr>
        <w:tc><w:p><w:r><w:t>Is Ontology Descriptor</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{{is_ongology_descriptor}}</w:t></w:r></w:p></w:tc>
      </w:tr>
      <w:tr>
        <w:tc><w:p><w:r><w:t>Can Be Held</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{{can_be_held}}</w:t></w:r></w:p></w:tc>
      </w:tr>
      <w:tr>
        <w:tc><w:p><w:r><w:t>Has Identity</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{{has_identity}}</w:t></w:r></w:p></w:tc>
      </w:tr>
      <w:tr>
        <w:tc><w:p><w:r><w:t>Distance From Concept</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{{distance_from_concept}}</w:t></w:r></w:p></w:tc>
      </w:tr>

      <!-- Calculated Fields -->
      <w:tr>
        <w:tc><w:p><w:pPr><w:shd w:val="clear" w:color="auto" w:fill="E0E0E0"/></w:pPr><w:r><w:rPr><w:b/></w:rPr><w:t>Calculated Fields</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:pPr><w:shd w:val="clear" w:color="auto" w:fill="E0E0E0"/></w:pPr><w:r><w:t></w:t></w:r></w:p></w:tc>
      </w:tr>

      <!-- Level 1 -->
      <w:tr>
        <w:tc><w:p><w:r><w:t>Category Contains Language (L1)</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{{category_contains_language}}</w:t></w:r></w:p></w:tc>
      </w:tr>
      <w:tr>
        <w:tc><w:p><w:r><w:t>Has Grammar (L1)</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{{has_grammar}}</w:t></w:r></w:p></w:tc>
      </w:tr>
      <w:tr>
        <w:tc><w:p><w:r><w:t>Relationship To Concept (L1)</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{{relationship_to_concept}}</w:t></w:r></w:p></w:tc>
      </w:tr>
      <w:tr>
        <w:tc><w:p><w:r><w:t>Family Feud Question (L1)</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{{family_fued_question}}</w:t></w:r></w:p></w:tc>
      </w:tr>

      <!-- Level 2 -->
      <w:tr>
        <w:tc><w:p><w:r><w:t>Is A Family Feud Top Answer (L2)</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{{is_a_family_feud_top_answer}}</w:t></w:r></w:p></w:tc>
      </w:tr>

      <!-- Level 3 -->
      <w:tr>
        <w:tc><w:p><w:r><w:t>Family Feud Mismatch (L3)</w:t></w:r></w:p></w:tc>
        <w:tc><w:p><w:r><w:t>{{family_feud_mismatch}}</w:t></w:r></w:p></w:tc>
      </w:tr>
    </w:tbl>

    <w:p/>
    <!-- {{/each}} -->

    <!-- DAG Summary -->
    <w:p>
      <w:pPr><w:pStyle w:val="Heading1"/></w:pPr>
      <w:r><w:t>DAG Execution Order</w:t></w:r>
    </w:p>
    <w:p>
      <w:pPr><w:pStyle w:val="CodeBlock"/></w:pPr>
      <w:r><w:t>Level 0 (Raw):     All base table fields</w:t></w:r>
    </w:p>
    <w:p>
      <w:pPr><w:pStyle w:val="CodeBlock"/></w:pPr>
      <w:r><w:t>Level 1 (Simple):  category_contains_language, has_grammar, relationship_to_concept, family_fued_question</w:t></w:r>
    </w:p>
    <w:p>
      <w:pPr><w:pStyle w:val="CodeBlock"/></w:pPr>
      <w:r><w:t>Level 2 (Dependent): is_a_family_feud_top_answer</w:t></w:r>
    </w:p>
    <w:p>
      <w:pPr><w:pStyle w:val="CodeBlock"/></w:pPr>
      <w:r><w:t>Level 3 (Higher):  family_feud_mismatch</w:t></w:r>
    </w:p>

  </w:body>
</w:document>
