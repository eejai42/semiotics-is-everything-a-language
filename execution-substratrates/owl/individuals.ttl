erb:IsEverythingALanguage_6 a erb:IsEverythingALanguage ;
    erb:isEverythingALanguageId "neial-007" ;
    erb:name "NEIAL-007" ;
    erb:argumentName "NotEverythingIsALanguage" ;
    erb:argumentCategory "Premise" ;
    erb:stepType "Entailment" ;
    erb:statement "If Language(x) is defined by necessary conditions, then any x that fails at least one condition is not a language." ;
    erb:formalization "∀x (¬(HasSyntax(x) ∧ RequiresParsing(x) ∧ HasLinearDecodingPressure(x) ∧ StableOntologyReference(x)) → ¬Language(x))" ;
    erb:relatedCandidateName "" ;
    erb:relatedCandidateId "" ;
    erb:evidenceFromRulebook "" ;
    erb:notes "This is the standard move: a definition implies exclusions." .

