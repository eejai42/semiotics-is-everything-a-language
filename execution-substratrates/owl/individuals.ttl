erb:IsEverythingALanguage_2 a erb:IsEverythingALanguage ;
    erb:isEverythingALanguageId "neial-003" ;
    erb:name "NEIAL-003" ;
    erb:argumentName "LanguageCanBeFormalized" ;
    erb:argumentCategory "Definition" ;
    erb:stepType "Definition" ;
    erb:statement "Operational definition: an item x counts as a Language if it has syntax, requires parsing, serializes meaning, and functions as an ontology/descriptor system." ;
    erb:formalization "Language(x) := HasSyntax(x) ∧ RequiresParsing(x) ∧ HasLinearDecodingPressure(x) ∧ StableOntologyReference(x)" ;
    erb:relatedCandidateName "" ;
    erb:relatedCandidateId "" ;
    erb:evidenceFromRulebook "" ;
    erb:notes "This is a deliberately 'strong' definition: it matches formal languages and many natural language representations." .

